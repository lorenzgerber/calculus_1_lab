\section{Code Listings}
All calculations were done in R \cite{rlanguage}.
\subsection{nderiv}
\begin{lstlisting}
  nderiv = function (f, x, h) (f(x+h)-f(x))/h
\end{lstlisting}

\subsection{newton}
\begin{lstlisting}
  newton = function (f, x, n) {
    for(i in 1:n){
      x <- x-f(x)/nderiv(f,x,0.0001)
    }
    return(x)
  }
\end{lstlisting}



